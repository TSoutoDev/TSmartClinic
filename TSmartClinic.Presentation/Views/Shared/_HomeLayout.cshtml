    <style>
        .clickable:hover {
            cursor: pointer;
        }

        .card-header h5 {
            position: relative;
            display: inline-block;
        }

            .card-header h5::after {
                content: '';
                display: block;
                width: 250%;
                height: 1px;
                background: linear-gradient(to right, #dee2e6, rgba(222,226,230,0));
                position: absolute;
                bottom: -10px;
                left: 0;
            }

        .titulo-com-icone {
            display: flex;
            align-items: center;
            font-size: 28px !important; /* Força o tamanho do texto */
            font-weight: 600 !important; /* Força o peso */
        }


        .icone-titulo {
            font-size: 34px; /* Tamanho do ícone */
            color: #6c757d; /* Cinza */
        }



        html, body {
            height: 100%;
            margin: 0;
            padding: 0;
        }

        #layout-wrapper {
            display: flex;
            flex-direction: column;
            height: 100vh; /* Ocupa a altura total da tela */
        }

        .main-content {
            flex: 1; /* Ocupa todo o espaço disponível */
            display: flex;
            flex-direction: column;
            min-height: 0; /* Permite que o conteúdo seja rolável */
        }

        .page-title-box h4 {
            position: relative;
            z-index: 5;
        }

        .page-content {
            flex: 1;
            overflow-y: auto; /* HABILITA A ROLAGEM */
            padding: 1rem;
        }

        footer.footer {
            flex-shrink: 0; /* Não deixa o footer colapsar */
            background-color: #f8f9fa;
            padding: 16px;
            text-align: center;
            border-top: 1px solid #dee2e6;
        }

    </style>

</head>

